      "target": "api::category.category",
      "inversedBy": "restaurants"
    },
    "uid": {
      "type": "uid",
      "targetField": "Name"
    },
    "logo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
